DEEPMD_ROOT=@CMAKE_INSTALL_PREFIX@
TENSORFLOW_INCLUDE_DIRS="@TensorFlow_INCLUDE_DIRS@"
TENSORFLOWFRAMEWORK_DIRS="@TensorFlow_LIBRARY_PATH@"
TENSORFLOWFRAMEWORK_PATH="@TensorFlowFramework_LIBRARY@"
TENSORFLOW_LIBRARY_PATH="@TensorFlow_LIBRARY@"

NNP_INC=" -std=c++@CMAKE_CXX_STANDARD@ -D@prec_def@ @TTM_DEF@ -DLAMMPS_VERSION_NUMBER=@LAMMPS_VERSION_NUMBER@ -DHUAWEI_ASCEND -I$TENSORFLOW_INCLUDE_DIRS -I$DEEPMD_ROOT/include/ "
NNP_PATH=" $TENSORFLOW_LIBRARY_PATH -L$DEEPMD_ROOT/lib -L$TENSORFLOWFRAMEWORK_DIRS"
NNP_LIB=" -Wl,--no-as-needed -l@LIB_DEEPMD_CC@@variant_name@ -ltensorflow_framework -Wl,-rpath=$DEEPMD_ROOT/lib"